package pls.othertools.rvptool;

import org.jfree.data.xy.XYDataItem;

@SuppressWarnings("serial")
public class RvpDataItem extends XYDataItem {

	private String mPcValue = ""; 
	
	public RvpDataItem(double x, double y, String pcvalue) {
		super(x, y);
		
		mPcValue = pcvalue;
	}
	
	public String getPcValue() {
		return mPcValue;
	}
}
